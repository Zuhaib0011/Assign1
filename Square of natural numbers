#include <stdio.h>
#include <math.h>
void sqn_series(int a,int b,int c)
{
float g=0.5;
int d=pow(a,g);
int e=pow(b,g);
int f=pow(c,g);
if (b-a!=d+e&&c-b!=f+e)
{
    printf("It is not\n");
    }

    else 
    
    {
        printf("It is\n");}
    }
    
    

int main()
{
    int n1,n2,n3;
    printf("Enter 3 numbers\n");
    scanf("%d%d%d",&n1,&n2,&n3);
    sqn_series(n1,n2,n3);
    return 0;
}
